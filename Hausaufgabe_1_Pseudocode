INPUT number1
INPUT number2
INPUT operator

IF operator = "+" THEN
    result = number1 + number2
ELSE IF operator = "-" THEN
    result = number1 - number2
ELSE IF operator = "*" THEN
    result = number1 * number2
ELSE IF operator = "/" THEN
    result = number1 / number2
ELSE
    OUTPUT "Bitte Operator eingeben"
END IF

OUTPUT result


INPUT score

IF score >= 90 THEN
    OUTPUT "Grade A"
ELSE IF score >= 80 THEN
    OUTPUT "Grade B"
ELSE IF score >= 70 THEN
    OUTPUT "Grade C"
ELSE IF score >= 60 THEN
    OUTPUT "Grade D"
ELSE
    OUTPUT "Grade F"
END IF