
number1 = INPUT
number2 = INPUT
operator = INPUT

IF operator = "+"
    result = number1 + number2
ELSE IF operator = "-"
    result = number1 - number2
ELSE IF operator = "*"
    result = number1 * number2
ELSE IF operator = "/"
    result = number1 / number2
ELSE 
    OUTPUT "Bitte Operator angeben"
END IF
OUTPUT result



